# prwd config file example

# Maximum size of the returned working directory
set maxlength 24
set filler "â€¦"
set cleancut on

# Shorten the path newsgroup style (first letters)
#set newsgroup on

# Show mercurial and git branches
#set mercurial on
#set git on

# Prefix the result with the short hostname.
#set hostname on

# Add a suffix character UID hint ($ or #).
#set uid_indicator on

# Aliases
alias *lt /home/tamentis/livetech
alias *svn /home/tamentis/dev/trunk
